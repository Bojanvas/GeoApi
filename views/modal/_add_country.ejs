<!-- Modal -->
<div id="add-country-modal" class="modal fade" role="dialog">
        <div class="modal-dialog">
        <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Add Country</h4>
                </div>
                <form id='add-country' enctype="multipart/form-data" method="post" name="fileinfo">
                    <div class="modal-body">
                        <div class="form-row">
                            <div class="form-group col">
                                <input type="text" name='country_name' class="form-control" id="country_name" aria-describedby="Country" placeholder="Country name">
                            </div>
                            <div class="form-group col">
                                <input type="text" name='capital' class="form-control" id="country_capital" aria-describedby="Capital" placeholder="Country capital">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col-5 form-group">
                                <input type="file" id='fileinfo' class="file-input-flag" name='flag_file' id="inputflag"
                                    aria-describedby="flag">
                            </div>
                            <div class="col-5 form-group">
                                <input type="file" class="file-input-country" name='country_file' id="inputcountry"
                                        aria-describedby="country">
                            </div>
                            <div class="col-2 form-group">
                                <select id="inputPoints" name="points" class="form-control">
                                    <option>5</option>
                                    <option>6</option>
                                    <option>7</option>
                                    <option>8</option>
                                    <option>9</option>
                                    <option>10</option>
                                </select>
                            </div>
                        </div>
                        <div class="spinner-border m-auto d-none" role="status">
                            <span class="sr-only">Loading...</span>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                        <input type='submit' class="btn btn-primary" id='add-new-country-js' data-dismiss=""></input>
                    </div>
                </form>
            </div>
        </div>
    </div>
    